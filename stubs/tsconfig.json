{
    "compilerOptions": {
        "types": ["vite/client"],
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "isolatedModules": true,
        "baseUrl": "./",
        "paths": {
            "@Pages/*": ["./resources/$PAGESPATH$/*"],
            "@Components/*": ["./resources/$COMPONENTSPATH$/*"]
        }
    }
}
